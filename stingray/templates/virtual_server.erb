<% if @request_rules then -%>
rules <%= @request_rules.class == Array ? "\"" + (@request_rules * "\" \"") + "\"" : "\"" + @request_rules  + "\""%>
<% end -%>
<% if @protection then -%>protection  <%= @protection %>
<% end -%>
<% if @bandwidth then -%>bandwidth_class  <%= @bandwidth %>
<% end -%>
<% if @timeout -%>timeout <%= @timeout %>
<% end -%>
<% if @connect_timeout -%>connect_time  <%= @connect_timeout %>
<% end -%>
<% if @compression_level then -%>gzip!compresslevel  <%= @compression_level %>
